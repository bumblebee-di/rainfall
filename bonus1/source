int main (int argc, char **argv) {

    char str[40];
    int num = atoi(argv[1]);

    if (num <= 9) { // logically as unsigned int
        memcpy(str, argv[2], num * 4);
        if (0x574f4c46 == num) // as unsigned int
            execl("/bin/sh", "sh", 0);
    }
    else 
        return (1);
    return (0);
}