#include <stdio.h>
#include <stdlib.h>
#include <string.h>

void m() {
    puts("Nope");
}

void n() {
    system("/bin/cat /home/user/level7/.pass");
}

int main(int argc, char *argv) {

    void (*func_ptr)(void);
    char *str = (char *)malloc(64);

    func_ptr = (void *)malloc(4);

    func_ptr = &m;
    
    strcpy(str, &argv[4]);

    func_ptr();
    return 0;
}